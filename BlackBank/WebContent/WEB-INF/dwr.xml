<!DOCTYPE dwr PUBLIC "-//GetAhead Limited//DTD Direct Web Remoting 1.0//EN" "http://www.getahead.ltd.uk/dwr/dwr10.dtd">
<dwr>
	<allow>
		<convert converter="bean" match="bank.model.teller.TellerBean" />
		
		<create creator="new" javascript="ReadTeller">
			<param name="class" value="bank.model.teller.action.ReadTeller" />			
			<include method="helloName" />			
		</create>
		
		<create creator="new" javascript="CodeCheck">
			<param name="class" value="bank.model.product.action.CodeCheck" />			
			<include method="prdCode" />			
		</create>
		
	</allow>
</dwr> 